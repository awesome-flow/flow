{
	"version": "v1alpha1",
	"components": {
		"listener": {
			"module": "listener",
			"params": {
				"bind": ":10000"
			}
		},
		"grep": {
			"module": "grep",
			"params": {
				"mask": "llo"
			}
		},
		"dumper": {
			"module": "dumper",
			"params": {
				"out": "STDOUT"
			}
		}
	},
	"pipeline": {
		"listener": {
			"connect": "grep"
		},
		"grep": {
			"connect": "dumper"
		}
	}
}
